datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output   = "../prisma/generated/type-graphql"
}

model Tool {
  id          Int     @id @default(autoincrement())
  name        String  @unique
  description String
  link        String
  image       String?
}

